{
  "manifest_version": 2,
  "name": "Quora To WordPress",
  "version": "1.0",
  "description": "Save Quora Question and Answers to Wordpress",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.quora.com/*"],
      "js": ["content.js"]
    }
  ]
}
