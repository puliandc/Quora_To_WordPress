{
  "manifest_version": 2,
  "name": "Quora To WordPress",
  "version": "1.0",
  "description": "Save Quora Question and Answers to Wordpress",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "clipboardWrite"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.quora.com/*"],
      "js": ["content.js"]
    }
  ]
}
